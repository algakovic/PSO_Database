#!/usr/bin/env python
'''Program to interact with PSO_DATABASE'''

import sqlite3

conn = sqlite3.connect('pso_database.db')
c = conn.cursor()

def print_rows(table):
    print(table)
    for row in c.execute(f'SELECT * FROM {table}'):
        print(row)
    print('\n')

print_rows('Mags')
print_rows('Materials')
print_rows('Units')
