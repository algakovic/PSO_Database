#!/usr/bin/env python
'''Program to interact with PSO_DATABASE'''

import sqlite3

conn = sqlite3.connect('pso_database.db')
c = conn.cursor()

def print_tables(table):
    print(table)
    for table in c.execute(f'SELECT * FROM {table}'):
        print(table)
    print('\n')

print_tables('Mags')
print_tables('Materials')
print_tables('Units')
